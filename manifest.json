{
   "background": {
      "persistent": false,
      "scripts": [ "scripts/common.js", "scripts/storage.js", "scripts/activity.js", "scripts/tab.js", "scripts/timeInterval.js", "scripts/background.js","scripts/ui.js" ]
   },
   "browser_action": {
      "default_icon": "icons/egg48.png",
      "default_popup": "index.html",
      "default_title": "Time Tracker Monster"
   },
   "description": "Track Monster",
   "icons": {
      "128": "icons/egg128.png",
      "16": "icons/egg16.png",
      "32": "icons/egg32.png",
      "48": "icons/egg48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "26",
   "name": "Time Tracker Monster",
   "short_name": "Time Tracker Monster",
   "version": "1.0.1",
   "permissions": [ "tabs", "activeTab", "storage", "idle", "chrome://favicon/*", "webNavigation", "unlimitedStorage" ],
   "offline_enabled": true,
   "options_page": "options.html"
}